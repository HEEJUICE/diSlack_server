{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "import/no-dynamic-require": 0,
    "consistent-return": 0,
    "camelcase": 0,
    "no-param-reassign": 0,
    "no-await-in-loop": 0
  }
}
